        "16": "1.png",
        "32": "2.png",
        "48": "3.png",
        "128": "4.png"
      }
    },
    "icons": {
      "16": "images/1.png",
        "32": "images/2.png",
        "48": "images/3.png",
        "128": "images/4.png"
    },
    "manifest_version": 2
  }