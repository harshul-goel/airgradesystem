  {
    "name": "Air Grade System",
    "version": "1.0",
    "description": "Get to know the air quality of anywhere.",
    "permissions": ["activeTab","declarativeContent","storage","webNavigation","geolocation"],
    "content_scripts":[{"js":["jquery-3.3.1.min.js"],"matches": ["http://*/*", "https://*/*"]}],
    "background": {
      "scripts": ["jquery-3.3.1.min.js"],
      "persistent": false
